{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "yarn pod-install",
    "pod-install": "pod-install ios && pod-install macos",
    "lint": "eslint .",
    "test": "jest",
    "start": "react-native start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "start:macos": "react-native run-macos",
    "reset": "rm -rf node_modules && yarn reset:ios && yarn reset:macos && yarn",
    "reset:ios": "rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock;",
    "reset:macos": "rm -rf macos/build; rm -rf macos/Pods; rm -rf macos/Podfile.lock;"
  },
  "dependencies": {
    "@op-engineering/op-sqlite": "https://github.com/thejustinwalsh/op-sqlite.git#macos-support",
    "@tamagui/animations-react-native": "^1.79.2",
    "@tamagui/config": "^1.79.2",
    "@tamagui/lucide-icons": "^1.79.2",
    "@tamagui/shorthands": "^1.79.2",
    "@tamagui/themes": "^1.79.2",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-macos": "^0.72.0-0",
    "react-native-svg": "^14.0.0",
    "react-native-web": "^0.19.9",
    "tamagui": "^1.79.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "pod-install": "^0.1.39",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.212.0",
    "react-native-flipper-databases": "^2.5.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
